@page "/tooltips/{Mode=static}"
@using BsBlazor.Demo.Client.SharedDemo.Tooltips
<PageTitle>Tooltips - @Mode</PageTitle>
@switch (Mode)
{
    case "server": <SharedTooltipDemo @rendermode="InteractiveServer"/> break;
    case "wasm": <SharedTooltipDemo @rendermode="InteractiveWebAssembly"/> break;
    default: <SharedTooltipDemo /> break;
}
@code { [Parameter] public string? Mode { get; set; } }